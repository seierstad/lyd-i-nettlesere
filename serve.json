{
  "headers": [
    {
      "source" : "**/*.@(jpg|jpeg|gif|png)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=7200"
      }]
    }, {
      "source" : "**/*.@(html|js|css)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "no-store"
      }]
    }
  ]
}
